# -*- coding: utf-8 -*-

def cmp_ignore_case(s1, s2):
    s1 = s1.lower()
    s2 = s2.lower()
    if s1>s2:
      return 1
    elif s1<s2:
      return -1
    return 0
    
def reverse_cmp(x, y):
  if x > y:
    return -1
  elif x < y:
    return 1
  return 0

print sorted(['bob', 'about', 'Zoo', 'Credit'], cmp_ignore_case)
print sorted([36, 5, 12, 9, 21])  # default sorted,  from small --> big
print sorted([36, 5, 12, 9, 21], reversed_cmp)  # use reverse sort  
